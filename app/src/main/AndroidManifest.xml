<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.prototype13">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application android:theme="@style/BaseAppTheme"
        android:supportsRtl="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:allowBackup="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".LoginActivity"/>
        <activity android:name=".BrowserActivity"/>
        <activity android:name=".ProductPageActivity"/>
        <activity android:name=".ProductGridActivity"/>
        <activity android:name=".OnBoardingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".HomePageActivity"/> </application>

</manifest>